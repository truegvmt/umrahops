{
    "version": 2,
    "builds": [
        {
            "src": "server/index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/public/**",
                    "data/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "server/index.ts"
        }
    ]
}